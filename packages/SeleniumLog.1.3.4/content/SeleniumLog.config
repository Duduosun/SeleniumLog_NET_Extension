<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  
  <enable_seleniumlog>true</enable_seleniumlog>
  <enable_selenium_webdriver_trace>true</enable_selenium_webdriver_trace>
  <enable_general_function_trace>true</enable_general_function_trace>
  
  <timestamp_format>HH:mm:ss.fff</timestamp_format>
  <output_file_path>C:/Tmp/log3.txt</output_file_path>
  <screenshots_folder>C:/tmp/screenshots</screenshots_folder>

  <screenshot_on_writeline>true</screenshot_on_writeline>
  <screenshot_on_pass>true</screenshot_on_pass>
  <screenshot_on_fail>true</screenshot_on_fail>
  <screenshot_on_error>true</screenshot_on_error>
  <screenshot_on_warning>true</screenshot_on_warning>

  <force_throw_exception_on_assert_fail>false</force_throw_exception_on_assert_fail>

  <on_click_event>
    <log_function_start>true</log_function_start>
    <log_function_end>false</log_function_end>
    <screenshot_on_start>true</screenshot_on_start>
    <screenshot_on_end>true</screenshot_on_end>      
  </on_click_event>

  <general_function_trace_display_null_inputs>true</general_function_trace_display_null_inputs>

</configuration>